{
  "name": "p5_smaple",
  "manifest_version": 2,
  "version": "1.0",
  "description": "p5jsを拡張機能に召喚しようぜ！",
  "icons": {
    "16": "icon/icon_16.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "browser_action": {
    "default_title": "p5_smaple",
    "default_icon": {
      "19": "icon/icon_19.png",
      "38": "icon/icon_38.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/p5.min.js", "js/sketch.js"],
      "css": ["css/style.css"]
    }
  ]
}
